#include<stdio.h>

void ft_isalpha(char *str)
{
    int i;

    i = 0;
    while (str[i] != '\0')
    {
        if (str[i] >= 97 && str[i] <= 122)
        {
            str[i] = str[i - 32];
            i++;
        }
    }
    printf("%s", str);
}

int main(void)
{
    char *str;

    str = "Hello";
    ft_isalpha(str);
    return (0);
}